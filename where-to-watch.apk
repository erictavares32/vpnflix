\n    return suggestion\n  } catch (error) {\n    console.error(\"Error getting Google spelling suggestion:\", error)\n    return null\n  }\n}\n\n/**\n * Enhanced suggestion function that combines our dictionary with Google suggestions\n * @param query The search query to check\n * @returns The best correction suggestion or null\n */\nexport async function getEnhancedSuggestion(query: string): Promise\u003cstring | null\u003e {\n  // First try our local dictionary for immediate response\n  const { suggestCorrection } = await import(\"./search-utils\")\n  const localSuggestion = suggestCorrection(query)\n\n  // If we have a local suggestion, use it\n  if (localSuggestion \u0026\u0026 localSuggestion !== query.toLowerCase()) {\n    return localSuggestion\n  }\n\n  // Otherwise, try Google's API for more advanced suggestions\n  // Only call Google if we don't have a local suggestion to minimize API usage\n  return getGoogleSpellingSuggestion(query)\n}\n"])</script><script>self.__next_f.push([1,"2b:T1b54,"])</script><script>self.__next_f.push([1,"\"use client\"\n\nimport { useState, useEffect } from \"react\"\nimport { Tooltip, TooltipContent, TooltipProvider, TooltipTrigger } from \"@/components/ui/tooltip\"\n\ninterface Rating {\n  source: string\n  value: number | string\n  outOf?: number\n  color: string\n  logo?: string\n}\n\ninterface ContentRatingsProps {\n  tmdbRating: number\n  tmdbVoteCount?: number\n  imdbId?: string\n  title: string\n  year: string\n  type: \"movie\" | \"tv\"\n}\n\nexport function ContentRatings({ tmdbRating, tmdbVoteCount, imdbId, title, year, type }: ContentRatingsProps) {\n  const [isLoading, setIsLoading] = useState(true)\n  const [ratings, setRatings] = useState\u003cRating[]\u003e([])\n  const [error, setError] = useState\u003cstring | null\u003e(null)\n  const [isExpanded, setIsExpanded] = useState(false)\n\n  // Use useEffect for side effects\n  useEffect(() =\u003e {\n    // Generate simulated ratings based on TMDB rating\n    const