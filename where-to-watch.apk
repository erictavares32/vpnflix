": 368,\n  \"NBA TV\": 436,\n}\n\n// Map country codes to JustWatch locale\nconst localeMapping: Record\u003cstring, string\u003e = {\n  US: \"en_US\",\n  GB: \"en_GB\",\n  CA: \"en_CA\",\n  AU: \"en_AU\",\n  DE: \"de_DE\",\n  FR: \"fr_FR\",\n  JP: \"ja_JP\",\n  IN: \"en_IN\",\n  BR: \"pt_BR\",\n  MX: \"es_MX\",\n  ES: \"es_ES\",\n  IT: \"it_IT\",\n  KR: \"ko_KR\",\n  RU: \"ru_RU\",\n  NL: \"nl_NL\",\n  TR: \"tr_TR\",\n  SE: \"sv_SE\",\n  ZA: \"en_ZA\",\n  AR: \"es_AR\",\n  SG: \"en_SG\",\n}\n\n// Flag to control whether to attempt JustWatch API calls\n// Set to false to disable JustWatch integration temporarily\nconst ENABLE_JUSTWATCH = false\n\n// Get JustWatch locale from country code\nfunction getLocale(countryCode: string): string {\n  return localeMapping[countryCode] || \"en_US\"\n}\n\n// Get JustWatch provider logo URL\nfunction getProviderLogo(providerId: number): string {\n  return `https://images.justwatch.com/icon/${providerId}/s100/icon.webp`\n}\n\n// Generate mock data for popular streaming services based on country\nfunction getMockStreamingData(country: string): JustWatchResult {\n  // Define popular services by country\n  const countryServices: Record\u003cstring, number[]\u003e = {\n    US: [8, 9, 337, 15, 384], // Netflix, Amazon, Disney+, Hulu, HBO Max\n    GB: [8, 9, 337, 39, 29], // Netflix, Amazon, Disney+, Now TV, Sky Go\n    CA: [8, 9, 337, 230, 384], // Netflix, Amazon, Disney+, Crave, HBO Max\n    AU: [8, 9, 337, 385, 21], // Netflix, Amazon, Disney+, Binge, Stan\n    // Default to US services for other countries\n  }\n\n  // Get services for the country or default to US\n  const services = countryServices[country] || countryServices.US\n\n  // Create mock providers\n  const providers: JustWatchProvider[] = []\n\n  // Map provider IDs to names\n  const providerNames: Record\u003cnumber, string\u003e = {\n    8: \"Netflix\",\n    9: \"Amazon Prime Video\",\n    337: \"Disney+\",\n    15: \"Hulu\",\n    384: \"HBO Max\",\n    39: \"Now TV\",\n    29: \"Sky Go\",\n    230: \"Crave\",\n    385: \"Binge\",\n    21: \"Stan\",\n  }\n\n  // Add subscription services\n  services.forEach((serviceId) =\u003e {\n    providers.push({\n      id: serviceId,\n      name: providerNames[serviceId] || `Provider ${serviceId}`,\n      logo: getProviderLogo(serviceId),\n      type: \"subscription\",\n    })\n  })\n\n  return {\n    id: \"mock-data\",\n    title: \"Mock Data\",\n    providers,\n  }\n}\n\n// Search for a title on JustWatch\nexport async function searchJustWatch(\n  title: string,\n  releaseYear: string | null,\n  countryCode: string,\n): Promise\u003cJustWatchResult | null\u003e {\n  // If JustWatch integration is disabled, return null\n  if (!ENABLE_JUSTWATCH) {\n    console.log(\"JustWatch integration is disabled, skipping API call\")\n    return null\n  }\n\n  try {\n    // Sanitize the title to remove special characters\n    const sanitizedTitle = title.replace(/[^\\w\\s]/gi, \"\").trim()\n\n    if (!sanitizedTitle) {\n      console.log(\"Empty title after sanitization, skipping JustWatch search\")\n      return null\n    }\n\n    const locale = getLocale(countryCode)\n\n    // Try the newer API endpoint format\n    const url = `https://apis.justwatch.com/content/titles/${locale}/popular`\n\n    console.log(`Searching JustWatch for \"${sanitizedTitle}\" (${releaseYear}) in ${locale}`)\n\n    // Build the query parameters - keep it simple to increase chances of success\n    const params = {\n      query: sanitizedTitle,\n      content_types: [\"movie\", \"show\"],\n    }\n\n    // Add release year if available, with a wider range\n    if (releaseYear \u0026\u0026 !isNaN(Number(releaseYear))) {\n      const year = Number(releaseYear)\n      params.release_year_from = year - 2\n      params.release_year_until = year + 2\n    }\n\n    const response = await fetch(url, {\n      method: \"POST\",\n      headers: {\n        \"Content-Type\": \"application/json\",\n      },\n      body: JSON.stringify(params),\n    })\n\n    if (!response.ok) {\n      console.error(`JustWatch API error: ${response.status} ${response.statusText}`)\n      return null\n    }\n\n    const data = await response.json()\n\n    if (!data.items || data.items.length === 0) {\n      console.log(`No JustWatch results found for \"${sanitizedTitle}\"`)\n      return null\n    }\n\n    // Get the first result\n    const result = data.items[0]\n    console.log(`Found JustWatch result: ${result.title} (${result.id})`)\n\n    // Extract providers\n    const providers: JustWatchProvider[] = []\n\n    if (result.offers \u0026\u0026 Array.isArray(result.offers)) {\n      result.offers.forEach((offer: any) =\u003e {\n        // Determine offer type\n        let type: \"subscription\" | \"rent\" | \"buy\" | \"free\" = \"subscription\"\n\n        if (offer.monetization_type === \"flatrate\") {\n          type = \"subscription\"\n        } else if (offer.monetization_type === \"rent\") {\n          type = \"rent\"\n        } else if (offer.monetization_type === \"buy\") {\n          type = \"buy\"\n        } else if (offer.monetization_type === \"free\") {\n          type = \"free\"\n        }\n\n        // Add provider if not already added for this type\n        const existingProvider = providers.find((p) =\u003e p.id === offer.provider_id \u0026\u0026 p.type === type)\n\n        if (!existingProvider \u0026\u0026 offer.provider_id) {\n          providers.push({\n            id: offer.provider_id,\n            name: offer.provider_name || `Provider ${offer.provider_id}`,\n            logo: getProviderLogo(offer.provider_id),\n            type,\n          })\n        }\n      })\n    } else {\n      console.log(`No offers found for \"${result.title}\"`)\n    }\n\n    return {\n      id: result.id.toString(),\n      title: result.title,\n      providers,\n    }\n  } catch (error) {\n    console.error(\"JustWatch API error:\", error)\n    return null\n  }\n}\n\n// Convert JustWatch providers to our format\nexport function convertJustWatchProviders(justWatchResult: JustWatchResult | null): {\n  streamingServices: {\n    provider: string\n    logo: string\n    type: \"subscription\" | \"rent\" | \"buy\"\n  }[]\n} {\n  if (!justWatchResult || !justWatchResult.providers || !Array.isArray(justWatchResult.providers)) {\n    return { streamingServices: [] }\n  }\n\n  return {\n    streamingServices: justWatchResult.providers\n      .filter((p) =\u003e p.type !== \"free\") // Exclude free providers if needed\n      .map((provider) =\u003e ({\n        provider: provider.name,\n        logo: provider.logo,\n        type: provider.type === \"free\" ? \"subscription\" : provider.type,\n      })),\n  }\n}\n\n// Get streaming availability from JustWatch\nexport async function getJustWatchAvailability(\n  title: string,\n  releaseYear: string | null,\n  countryCode: string,\n): Promise\u003c{\n  streamingServices: {\n    provider: string\n    logo: string\n    type: \"subscription\" | \"rent\" | \"buy\"\n  }[]\n}\u003e {\n  try {\n    if (!title) {\n      console.log(\"Empty title provided to getJustWatchAvailability\")\n      return { streamingServices: [] }\n    }\n\n    // Try to get real data from JustWatch API\n    const result = await searchJustWatch(title, releaseYear, countryCode)\n\n    // If we got real data, use it\n    if (result) {\n      return convertJustWatchProviders(result)\n    }\n\n    // If JustWatch API is disabled or failed, return empty results\n    return { streamingServices: [] }\n  } catch (error) {\n    console.error(\"Error getting JustWatch availability:\", error)\n    return { streamingServices: [] }\n  }\n}\n"])</script><script>self.__next_f.push([1,"18:T2927,"])</script><script>self.__next_f.push([1,"// Watchmode API integration for enhanced streaming availability data\n\n// Environment variable for Watchmode API key\nconst WATCHMODE_API_KEY = process.env.WATCHMODE_API_KEY || \"\"\n\n// Base URL for Watchmode API\nconst WATCHMODE_BASE_URL = \"https://api.watchmode.com/v1\"\n\n// Flag to control whether to attempt Watchmode title search as fallback\n// Set to false to disable title search which is causing 400 errors\nconst ENABLE_TITLE_SEARCH_FALLBACK = false\n\n// Types for Watchmode API responses\ninterface WatchmodeTitle {\n  id: number\n  title: string\n  type: \"movie\" | \"tv_series\"\n  year: number\n  tmdb_id: number\n  tmdb_type: \"movie\" | \"tv\"\n}\n\ninterface WatchmodeSource {\n  source_id: number\n  name: string\n  type: string // \"sub\", \"rent\", \"buy\", \"free\", etc.\n  region: string\n  web_url: string\n  format: string\n  price: number | null\n  seasons: number | null\n  episodes: number | null\n}\n\n// Cache for Watchmode data\nconst watchmodeCache: Record\u003cstring, { data: any; timestamp: number }\u003e = {}\nconst CACHE_DURATION = 7 * 24 * 60 * 60 * 1000 // 7 days\n\n// Search for a title on Watchmode by TMDB ID\nexport async function searchWatchmodeByTmdbId(tmdbId: number, type: \"movie\" | \"tv\"): Promise\u003cWatchmodeTitle | null\u003e {\n  if (!WATCHMODE_API_KEY) {\n    console.log(\"Watchmode API key not found\")\n    return null\n  }\n\n  const cacheKey = `watchmode-title-${tmdbId}-${type}`\n  const now = Date.now()\n\n  // Check cache\n  if (watchmodeCache[cacheKey] \u0026\u0026 now - watchmodeCache[cacheKey].timestamp \u003c CACHE_DURATION) {\n    return watchmodeCache[cacheKey].data\n  }\n\n  try {\n    const tmdbType = type === \"movie\" ? \"movie\" : \"tv\"\n\n    // Use the correct endpoint format according to Watchmode documentation\n    const url = `${WATCHMODE_BASE_URL}/title/tmdb/${tmdbId}/${tmdbType}?apiKey=${WATCHMODE_API_KEY}`\n\n    const response = await fetch(url, { next: { revalidate: 604800 } }) // Cache for 1 week\n\n    // Handle 404 errors gracefully - this means Watchmode doesn't have this title\n    if (response.status === 404) {\n      console.log(`Watchmode doesn't have data for TMDB ID ${tmdbId} (${type})`)\n      // Cache the null result to avoid repeated lookups\n      watchmodeCache[cacheKey] = { data: null, timestamp: now }\n      return null\n    }\n\n    if (!response.ok) {\n      throw new Error(`Watchmode API error: ${response.status} ${response.statusText}`)\n    }\n\n    const data = await response.json()\n\n    // Cache the result\n    watchmodeCache[cacheKey] = { data, timestamp: now }\n\n    return data\n  } catch (error) {\n    console.error(\"Error searching Watchmode by TMDB ID:\", error)\n    // Don't cache errors (except 404s which are already cached)\n    return null\n  }\n}\n\n// Alternative method: Search for a title on Watchmode by title and year\n// Note: This function is currently disabled due to 400 errors\nexport async function searchWatchmodeByTitle(\n  title: string,\n  year: string | null,\n  type: \"movie\" | \"tv\",\n): Promise\u003cWatchmodeTitle | null\u003e {\n  // If title search fallback is disabled or no API key or title, return null\n  if (!ENABLE_TITLE_SEARCH_FALLBACK || !WATCHMODE_API_KEY || !title) {\n    return null\n  }\n\n  const cacheKey = `watchmode-search-${title}-${year}-${type}`\n  const now = Date.now()\n\n  // Check cache\n  if (watchmodeCache[cacheKey] \u0026\u0026 now - watchmodeCache[cacheKey].timestamp \u003c CACHE_DURATION) {\n    return watchmodeCache[cacheKey].data\n  }\n\n  try {\n    // Sanitize the title to avoid special characters that might cause issues\n    const sanitizedTitle = title.replace(/[^\\w\\s]/gi, \"\").trim()\n\n    if (!sanitizedTitle) {\n      console.log(\"Title was empty after sanitization\")\n      return null\n    }\n\n    // Build search parameters\n    const searchParams = new URLSearchParams({\n      apiKey: WATCHMODE_API_KEY,\n      search_field: \"name\",\n      search_value: sanitizedTitle,\n    })\n\n    // Add type filter if available\n    if (type === \"movie\") {\n      searchParams.append(\"types\", \"movie\")\n    } else if (type === \"tv\") {\n      searchParams.append(\"types\", \"tv_series\")\n    }\n\n    // Add year filter if available\n    if (year) {\n      searchParams.append(\"year\", year)\n    }\n\n    const url = `${WATCHMODE_BASE_URL}/search/?${searchParams.toString()}`\n\n    console.log(`Searching Watchmode by title: ${sanitizedTitle} (${year})`)\n\n    const response = await fetch(url, { next: { revalidate: 604800 } })\n\n    if (!response.ok) {\n      throw new Error(`Watchmode search API error: ${response.status} ${response.statusText}`)\n    }\n\n    const data = await response.json()\n\n    // If no results, return null\n    if (!data.title_results || data.title_results.length === 0) {\n      console.log(`No Watchmode results found for title: ${sanitizedTitle}`)\n      watchmodeCache[cacheKey] = { data: null, timestamp: now }\n      return null\n    }\n\n    // Find the best match\n    const bestMatch = data.title_results[0]\n    console.log(`Found Watchmode title by search: ${bestMatch.name}`)\n\n    // Cache the result\n    watchmodeCache[cacheKey] = { data: bestMatch, timestamp: now }\n\n    return bestMatch\n  } catch (error) {\n    console.error(\"Error searching Watchmode by title:\", error)\n    // Cache the error to prevent repeated failed lookups\n    watchmodeCache[cacheKey] = { data: null, timestamp: now }\n    return null\n  }\n}\n\n// Get streaming sources for a title from Watchmode\nexport async function getWatchmodeSources(\n  watchmodeId: number,\n  regions: string[] = [\"US\"],\n): Promise\u003cRecord\u003cstring, WatchmodeSource[]\u003e\u003e {\n  if (!WATCHMODE_API_KEY) {\n    console.log(\"Watchmode API key not found\")\n    return {}\n  }\n\n  const cacheKey = `watchmode-sources-${watchmodeId}-${regions.join(\",\")}`\n  const now = Date.now()\n\n  // Check cache\n  if (watchmodeCache[cacheKey] \u0026\u0026 now - watchmodeCache[cacheKey].timestamp \u003c CACHE_DURATION) {\n    return watchmodeCache[cacheKey].data\n  }\n\n  try {\n    // Fetch sources for all requested regions\n    const sourcesPromises = regions.map(async (region) =\u003e {\n      