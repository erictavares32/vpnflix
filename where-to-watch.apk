Url } from \"@/components/canonical-url\"\nimport { StructuredData } from \"@/components/structured-data\"\nimport HowToWatchClient from \"./HowToWatchClient\"\n\n// This enables static generation\nexport const dynamic = \"force-dynamic\" // Changed to force-dynamic to ensure it works in production\nexport const revalidate = 604800 // Revalidate once per week\n\n// Helper function to get the proper display name for platforms\nfunction getPlatformDisplayName(platformId: string): string {\n  const platformMap: Record\u003cstring, string\u003e = {\n    \"disney-plus\": \"Disney+\",\n    \"prime-video\": \"Prime Video\",\n    \"apple-tv-plus\": \"Apple TV+\",\n    \"paramount-plus\": \"Paramount+\",\n    \"hbo-max\": \"HBO Max\",\n    max: \"Max\",\n  }\n\n  return (\n    platformMap[platformId] ||\n    streamingServices.find((s) =\u003e s.id === platformId)?.name ||\n    platformId.replace(/-/g, \" \").replace(/\\b\\w/g, (l) =\u003e l.toUpperCase())\n  )\n}\n\n// Helper function to get platform color\nfunction getPlatformColor(platformId: string): string {\n  const colorMap: Record\u003cstring, string\u003e = {\n    netflix: \"bg-red-600\",\n    \"disney-plus\": \"bg-blue-700\",\n    \"prime-video\": \"bg-blue-500\",\n    max: \"bg-purple-800\",\n    hulu: \"bg-green-600\",\n    \"paramount-plus\": \"bg-blue-900\",\n    \"apple-tv-plus\": \"bg-gray-800\",\n    peacock: \"bg-indigo-600\",\n  }\n\n  return colorMap[platformId] || streamingServices.find((s) =\u003e s.id === platformId)?.color || \"bg-amber-500\"\n}\n\n// Generate metadata for the page\nexport async function generateMetadata({ params, searchParams }): Promise\u003cMetadata\u003e {\n  try {\n    const { title, platform } = params\n    const { id, type } = searchParams\n\n    if (!id || !type) {\n      return {\n        title: \"Content Not Found\",\n        description: \"The requested content could not be found.\",\n      }\n    }\n\n    let contentDetails\n    try {\n      if (type === \"movie\") {\n     