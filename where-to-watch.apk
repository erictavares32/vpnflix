"87:T4f0c,"])</script><script>self.__next_f.push([1,"// This file contains utility functions for interacting with the TMDB API\nimport { cacheManager, CACHE_DURATIONS } from \"@/lib/cache\"\n\n// Simple in-memory cache for runtime (will be mostly replaced by KV)\nconst runtimeCache: Record\u003cstring, { data: any; timestamp: number }\u003e = {}\nconst RUNTIME_CACHE_DURATION = 60 * 1000 // 1 minute runtime cache to prevent duplicate requests\n\n// Types for TMDB API responses\ninterface TMDBMovie {\n  id: number\n  title: string\n  poster_path: string | null\n  backdrop_path: string | null\n  vote_average: number\n  release_date: string\n  overview: string\n  genre_ids: number[]\n  original_language: string\n  popularity: number\n  adult: boolean\n  video: boolean\n}\n\ninterface TMDBShow {\n  id: number\n  name: string\n  poster_path: string | null\n  backdrop_path: string | null\n  vote_average: number\n  first_air_date: string\n  overview: string\n  genre_ids: number[]\n  original_language: string\n  popularity: number\n  origin_country: string[]\n}\n\ninterface TMDBSearchResult {\n  id: number\n  media_type: \"movie\" | \"tv\" | \"person\"\n  title?: string\n  name?: string\n  poster_path: string | null\n  backdrop_path: string | null\n  vote_average: number\n  release_date?: string\n  first_air_date?: string\n  overview: string\n  genre_ids: number[]\n  original_language: string\n  popularity: number\n}\n\n// Placeholder data as fallback\nconst trendingMoviesData = [\n  {\n    id: 1,\n    title: \"Dune: Part Two\",\n    poster_path: \"/placeholder.svg?height=450\u0026width=300\",\n    vote_average: 8.5,\n    release_date: \"2024-03-01\",\n  },\n  {\n    id: 2,\n    title: \"The Batman\",\n    poster_path: \"/placeholder.svg?height=450\u0026width=300\",\n    vote_average: 7.9,\n    release_date: \"2022-03-04\",\n  },\n  {\n    id: 3,\n    title: \"Oppenheimer\",\n    poster_path: \"/placeholder.svg?height=450\u0026width=300\",\n    vote_average: 8.4,\n    release_date: \"2023-07-21\",\n  },\n  {\n    id: 4,\n    title: \"Barbie\",\n    poster_path: \"/placeholder.svg?height=450\u0026width=300\",\n    vote_average: 7.3,\n    release_date: \"2023-07-21\",\n  },\n  {\n    id: 5,\n    title: \"Godzilla x Kong\",\n    poster_path: \"/placeholder.svg?height=450\u0026width=300\",\n    vote_average: 7.8,\n    release_date: \"2024-04-12\",\n  },\n  {\n    id: 6,\n    title: \"The Fall Guy\",\n    poster_path: \"/placeholder.svg?height=450\u0026width=300\",\n    vote_average: 7.5,\n    release_date: \"2024-05-03\",\n  },\n  {\n    id: 7,\n    title: \"Furiosa\",\n    poster_path: \"/placeholder.svg?height=450\u0026width=300\",\n    vote_average: 8.1,\n    release_date: \"2024-05-24\",\n  },\n  {\n    id: 8,\n    title: \"Kingdom of the Planet of the Apes\",\n    poster_path: \"/placeholder.svg?height=450\u0026width=300\",\n    vote_average: 7.7,\n    release_date: \"2024-05-10\",\n  },\n]\n\nconst popularShowsData = [\n  {\n    id: 1,\n    name: \"The Last of Us\",\n    poster_path: \"/placeholder.svg?height=450\u0026width=300\",\n    vote_average: 8.7,\n    first_air_date: \"2023-01-15\",\n  },\n  {\n    id: 2,\n    name: \"House of the Dragon\",\n    poster_path: \"/placeholder.svg?height=450\u0026width=300\",\n    vote_average: 8.3,\n    first_air_date: \"2022-08-21\",\n  },\n  {\n    id: 3,\n    name: \"The Mandalorian\",\n    poster_path: \"/placeholder.svg?height=450\u0026width=300\",\n    vote_average: 8.5,\n    first_air_date: \"2019-11-12\",\n  },\n  {\n    id: 4,\n    name: \"Stranger Things\",\n    poster_path: \"/placeholder.svg?height=450\u0026width=300\",\n    vote_average: 8.6,\n    first_air_date: \"2016-07-15\",\n  },\n  {\n    id: 5,\n    name: \"The Boys\",\n    poster_path: \"/placeholder.svg?height=450\u0026width=300\",\n    vote_average: 8.4,\n    first_air_date: \"2019-07-26\",\n  },\n  {\n    id: 6,\n    name: \"Fallout\",\n    poster_path: \"/placeholder.svg?height=450\u0026width=300\",\n    vote_average: 8.2,\n    first_air_date: \"2024-04-11\",\n  },\n  {\n    id: 7,\n    name: \"Shogun\",\n    poster_path: \"/placeholder.svg?height=450\u0026width=300\",\n    vote_average: 8.8,\n    first_air_date: \"2024-02-27\",\n  },\n  {\n    id: 8,\n    name: \"The Penguin\",\n    poster_path: \"/placeholder.svg?height=450\u0026width=300\",\n    vote_average: 8.0,\n    first_air_date: \"2024-09-19\",\n  },\n]\n\n// Base URL for TMDB API\nconst TMDB_BASE_URL = \"https://api.themoviedb.org/3\"\n\n// Helper function to safely get the API key - only use the server-side variable\nfunction getApiKey() {\n  // Only use the server-side environment variable\n  const apiKey = process.env.TMDB_API_KEY\n\n  if (!apiKey) {\n    console.error(\"TMDB_API_KEY is not defined in environment variables\")\n    return null\n  }\n\n  return apiKey\n}\n\n// Helper function to make API requests to TMDB\nasync function fetchFromTMDB(endpoint: string, params: Record\u003cstring, string\u003e = {}) {\n  const apiKey = getApiKey()\n\n  if (!apiKey) {\n    throw new Error(\"API key is missing\")\n  }\n\n  const url = new URL(`${TMDB_BASE_URL}${endpoint}`)\n\n  // Add API key\n  url.searchParams.append(\"api_key\", apiKey)\n\n  // Add additional params\n  Object.entries(params).forEach(([key, value]) =\u003e {\n    url.searchParams.append(key, value)\n  })\n\n  const cacheKey = `tmdb:${url.toString()}`\n  const now = Date.now()\n\n  // Check runtime cache first (to prevent duplicate requests in the same session)\n  if (runtimeCache[cacheKey] \u0026\u0026 now - runtimeCache[cacheKey].timestamp \u003c RUNTIME_CACHE_DURATION) {\n    return runtimeCache[cacheKey].data\n  }\n\n  // Check persistent KV cache\n  const cachedData = await cacheManager.get(cacheKey)\n  if (cachedData) {\n    // Update runtime cache\n    runtimeCache[cacheKey] = { data: cachedData, timestamp: now }\n    return cachedData\n  }\n\n  try {\n    // Add timeout to prevent hanging requests\n    const controller = new AbortController()\n    const timeoutId = setTimeout(() =\u003e controller.abort(), 7000) // 7 second timeout\n\n    const response = await fetch(url.toString(), {\n      next: { revalidate: 10800 }, // Cache for 3 hours\n      signal: controller.signal,\n    })\n\n    clearTimeout(timeoutId)\n\n    if (!response.ok) {\n      throw new Error(`TMDB API error: ${response.status} ${response.statusText}`)\n    }\n\n    const data = await response.json()\n\n    // Determine appropriate cache duration based on endpoint type\n    let cacheDuration = CACHE_DURATIONS.MEDIUM\n\n    // Use longer cache for details that rarely change\n    if (endpoint.includes(\"/movie/\") || endpoint.includes(\"/tv/\")) {\n      const year = data.release_date?.substring(0, 4) || data.first_air_date?.substring(0, 4)\n      const currentYear = new Date().getFullYear()\n\n      if (year \u0026\u0026 Number.parseInt(year) \u003c currentYear - 2) {\n        cacheDuration = CACHE_DURATIONS.VERY_LONG // Much longer for older content\n      } else {\n        cacheDuration = CACHE_DURATIONS.LONG\n      }\n    }\n\n    // Cache trending/popular data for longer\n    if (endpoint.includes(\"/trending/\") || endpoint.includes(\"/popular\")) {\n      cacheDuration = CACHE_DURATIONS.LONG\n    }\n\n    // Cache genre lists and configuration for very long time\n    if (endpoint.includes(\"/genre/\") || endpoint.includes(\"/configuration\")) {\n      cacheDuration = CACHE_DURATIONS.VERY_LONG\n    }\n\n    // Cache the response\n    await cacheManager.set(cacheKey, data, cacheDuration)\n\n    // Update runtime cache\n    runtimeCache[cacheKey] = { data, timestamp: now }\n\n    return data\n  } catch (error) {\n    console.error(\"Error fetching from TMDB:\", error)\n\n    // Check if we have stale runtime cache data we can use as fallback\n    if (runtimeCache[cacheKey]) {\n      console.log(`Using stale runtime cached data for ${endpoint} as fallback`)\n      return runtimeCache[cacheKey].data\n    }\n\n    throw error\n  }\n}\n\n// Function to fetch trending movies\nexport async function fetchTrendingMovies() {\n  try {\n    const data = await fetchFromTMDB(\"/trending/movie/week\")\n    return data.results.map((movie: TMDBMovie) =\u003e ({\n      id: movie.id,\n      title: movie.title,\n      poster_path: movie.poster_path,\n      backdrop_path: movie.backdrop_path,\n      vote_average: movie.vote_average,\n      release_date: movie.release_date,\n      overview: movie.overview,\n    }))\n  } catch (error) {\n    console.error(\"Failed to fetch trending movies, using placeholder data:\", error)\n    return trendingMoviesData\n  }\n}\n\n// Function to fetch popular TV shows\nexport async function fetchPopularShows() {\n  try {\n    const data = await fetchFromTMDB(\"/tv/popular\")\n    return data.results.map((show: TMDBShow) =\u003e ({\n      id: show.id,\n      name: show.name,\n      poster_path: show.poster_path,\n      backdrop_path: show.backdrop_path,\n      vote_average: show.vote_average,\n      first_air_date: show.first_air_date,\n      overview: show.overview,\n    }))\n  } catch (error) {\n    console.error(\"Failed to fetch popular shows, using placeholder data:\", error)\n    return popularShowsData\n  }\n}\n\n// Function to search for movies and TV shows\nexport async function searchMedia(query: string, type: \"movie\" | \"tv\" | \"multi\" = \"multi\") {\n  if (!query || query.trim() === \"\") {\n    return []\n  }\n\n  try {\n    const data = await fetchFromTMDB(`/search/${type}`, { query: encodeURIComponent(query) })\n\n    return data.results\n      .map((item: TMDBSearchResult) =\u003e {\n        // Format the result based on media type\n        if (item.media_type === \"movie\" || type === \"movie\") {\n          return {\n            id: item.id,\n            title: item.title,\n            poster_path: item.poster_path,\n            vote_average: item.vote_average,\n            release_date: item.release_date,\n            media_type: \"movie\",\n            overview: item.overview,\n          }\n        } else if (item.media_type === \"tv\" || type === \"tv\") {\n          return {\n            id: item.id,\n            title: item.name,\n            name: item.name,\n            poster_path: item.poster_path,\n            vote_average: item.vote_average,\n            release_date: item.first_air_date,\n            first_air_date: item.first_air_date,\n            media_type: \"tv\",\n            overview: item.overview,\n          }\n        }\n        return null\n      })\n      .filter(Boolean)\n  } catch (error) {\n    console.error(\"Failed to search media, using filtered placeholder data:\", error)\n\n    // Fallback to filtering placeholder data\n    const allMedia = [\n      ...trendingMoviesData.map((movie) =\u003e ({\n        ...movie,\n        media_type: \"movie\",\n      })),\n      ...popularShowsData.map((show) =\u003e ({\n        id: show.id,\n        title: show.name,\n        poster_path: show.poster_path,\n        vote_average: show.vote_average,\n        release_date: show.first_air_date,\n        media_type: \"tv\",\n      })),\n    ]\n\n    return allMedia.filter((item) =\u003e item.title.toLowerCase().includes(query.toLowerCase()))\n  }\n}\n\n// Add this new function to search for actors\nexport async function searchPeople(query: string) {\n  if (!query || query.trim() === \"\") {\n    return []\n  }\n\n  try {\n    // This uses fetchFromTMDB which will cache with our updated durations\n    const data = await fetchFromTMDB(`/search/person`, { query: encodeURIComponent(query) })\n\n    return data.results.map((person: any) =\u003e ({\n      id: person.id,\n      name: person.name,\n      profile_path: person.profile_path,\n      known_for_department: person.known_for_department,\n      popularity: person.popularity,\n      known_for: person.known_for || [],\n      media_type: \"person\",\n    }))\n  } catch (error) {\n    console.error(\"Failed to search people:\", error)\n    return []\n  }\n}\n\n// Function to get movie details by ID\nexport async function getMovieDetails(id: number) {\n  try {\n    return await fetchFromTMDB(`/movie/${id}`, { append_to_response: \"videos,credits,similar\" })\n  } catch (error) {\n    console.error(`Failed to get movie details for ID ${id}, using placeholder data:`, error)\n    return trendingMoviesData.find((movie) =\u003e movie.id === id) || null\n  }\n}\n\n// Function to get TV show details by ID\nexport async function getShowDetails(id: number) {\n  try {\n    return await fetchFromTMDB(`/tv/${id}`, { append_to_response: \"videos,credits,similar\" })\n  } catch (error) {\n    console.error(`Failed to get show details for ID ${id}, using placeholder data:`, error)\n    return popularShowsData.find((show) =\u003e show.id === id) || null\n  }\n}\n\n// Add this function to get person details\nexport async function getPersonDetails(id: number) {\n  try {\n    console.log(`[TMDB] Fetching person details for ID: ${id}`)\n\n    // Use the API key directly for this critical request\n    const apiKey = process.env.TMDB_API_KEY\n    if (!apiKey) {\n      console.error(\"[TMDB] API key is missing\")\n      return null\n    }\n\n    // Check if we have cached data\n    const cacheKey = `tmdb:person:${id}`\n    const cachedData = await cacheManager.get(cacheKey)\n    if (cachedData) {\n      console.log(`[TMDB] Using cached data for person ${id}`)\n      return cachedData\n    }\n\n    // Make sure we're requesting the combined_credits\n    const url = `https://api.themoviedb.org/3/person/${id}?api_key=${apiKey}\u0026append_to_response=movie_credits,tv_credits,combined_credits,images`\n\n    console.log(`[TMDB] Request URL: ${url}`)\n\n    // Use a direct fetch with longer timeout\n    const controller = new AbortController()\n    const timeoutId = setTimeout(() =\u003e controller.abort(), 15000) // 15 second timeout\n\n    const response = await fetch(url, {\n      signal: controller.signal,\n      cache: \"no-store\", // Ensure we're not using any cache\n    })\n\n    clearTimeout(timeoutId)\n\n    if (!response.ok) {\n      const errorText = await response.text()\n      console.error(`[TMDB] Error fetching person ${id}: ${response.status}`, errorText)\n      return null\n    }\n\n    const data = await response.json()\n\n    // Ensure we have the basic structure for credits\n    if (!data.movie_credits) {\n      data.movie_credits = { cast: [], crew: [] }\n    }\n    if (!data.tv_credits) {\n      data.tv_credits = { cast: [], crew: [] }\n    }\n\n    // Log the structure to help debug\n    console.log(`[TMDB] Person data structure:`, {\n      id: data.id,\n      name: data.name,\n      has_movie_credits: !!data.movie_credits,\n      movie_credits_cast_count: data.movie_credits?.cast?.length || 0,\n      tv_credits_cast_count: data.tv_credits?.cast?.length || 0,\n    })\n\n    // Cache person data for 30 days since it rarely changes\n    await cacheManager.set(cacheKey, data, CACHE_DURATIONS.ACTOR)\n\n    return data\n  } catch (error) {\n    console.error(`[TMDB] Error in getPersonDetails for ID ${id}:`, error)\n    return null\n  }\n}\n\n// Function to discover movies by genre, year, etc.\nexport async function discoverMovies(params: Record\u003cstring, string\u003e = {}) {\n  try {\n    const data = await fetchFromTMDB(\"/discover/movie\", params)\n    return data.results\n  } catch (error) {\n    console.error(\"Failed to discover movies:\", error)\n    return trendingMoviesData\n  }\n}\n\n// Function to discover TV shows by genre, year, etc.\nexport async function discoverShows(params: Record\u003cstring, string\u003e = {}) {\n  try {\n    const data = await fetchFromTMDB(\"/discover/tv\", params)\n    return data.results\n  } catch (error) {\n    console.error(\"Failed to discover shows:\", error)\n    return popularShowsData\n  }\n}\n\n// Function to get movie genres\nexport async function getMovieGenres() {\n  try {\n    const data = await fetchFromTMDB(\"/genre/movie/list\")\n    return data.genres\n  } catch (error) {\n    console.error(\"Failed to get movie genres:\", error)\n    return []\n  }\n}\n\n// Function to get TV show genres\nexport async function getTvGenres() {\n  try {\n    const data = await fetchFromTMDB(\"/genre/tv/list\")\n    return data.genres\n  } catch (error) {\n    console.error(\"Failed to get TV genres:\", error)\n    return []\n  }\n}\n\n// Function to get streaming availability for a movie or TV show\nexport async function getStreamingAvailability(id: number, type: \"movie\" | \"tv\", country = \"US\") {\n  try {\n    // Create a cache key for streaming availability\n    const cacheKey = `streaming:${type}:${id}:${country}`\n\n    // Check if we have cached data\n    const cachedData = await cacheManager.get(cacheKey)\n    if (cachedData) {\n      console.log(`Using cached streaming availability for ${type} ID ${id} in country ${country}`)\n      return cachedData\n    }\n\n    // TMDB has a \"watch/providers\" endpoint that gives provider info by country\n    const data = await fetchFromTMDB(`/${type}/${id}/watch/providers`)\n\n    // Extract providers for the requested country\n    const countryData = data.results[country]\n\n    if (!countryData) {\n      console.log(`No streaming data available for ${country}, returning empty array`)\n      const result = {\n        id,\n        type,\n        country,\n        streamingServices: [],\n      }\n\n      // Cache the empty result for 7 days\n      await cacheManager.set(cacheKey, result, CACHE_DURATIONS.VERY_LONG / 2) // 7 days\n\n      return result\n    }\n\n    // Format the providers data\n    const providers = []\n\n    if (countryData.flatrate) {\n      providers.push(\n        ...countryData.flatrate.map((provider: any) =\u003e ({\n          provider: provider.provider_name,\n          logo: provider.logo_path\n            ? `https://image.tmdb.org/t/p/original${provider.logo_path}`\n            : \"/placeholder.svg?height=50\u0026width=50\",\n          type: \"subscription\",\n        })),\n      )\n    }\n\n    if (countryData.rent) {\n      providers.push(\n        ...countryData.rent.map((provider: any) =\u003e ({\n          provider: provider.provider_name,\n          logo: provider.logo_path\n            ? `https://image.tmdb.org/t/p/original${provider.logo_path}`\n            : \"/placeholder.svg?height=50\u0026width=50\",\n          type: \"rent\",\n        })),\n      )\n    }\n\n    if (countryData.buy) {\n      providers.push(\n        ...countryData.buy.map((provider: any) =\u003e ({\n          provider: provider.provider_name,\n          logo: provider.logo_path\n            ? `https://image.tmdb.org/t/p/original${provider.logo_path}`\n            : \"/placeholder.svg?height=50\u0026width=50\",\n          type: \"buy\",\n        })),\n      )\n    }\n\n    const result = {\n      id,\n      type,\n      country,\n      streamingServices: providers,\n    }\n\n    // Cache the result for 7 days\n    await cacheManager.set(cacheKey, result, CACHE_DURATIONS.VERY_LONG / 2) // 7 days\n\n    return result\n  } catch (error) {\n    console.error(`Failed to get streaming availability for ${type} ID ${id} in country ${country}:`, error)\n\n    // Return empty array instead of placeholder data for better accuracy\n    return {\n      id,\n      type,\n      country,\n      streamingServices: [],\n    }\n  }\n}\n\n// Function to get a list of countries with their ISO codes\nexport function getCountries() {\n  return [\n    // North America\n    { code: \"US\", name: \"United States\" },\n    { code: \"CA\", name: \"Canada\" },\n    { code: \"MX\", name: \"Mexico\" },\n\n    // Europe\n    { code: \"GB\", name: \"United Kingdom\" },\n    { code: \"DE\", name: \"Germany\" },\n    { code: \"FR\", name: \"France\" },\n    { code: \"ES\", name: \"Spain\" },\n    { code: \"IT\", name: \"Italy\" },\n    { code: \"NL\", name: \"Netherlands\" },\n    { code: \"SE\", name: \"Sweden\" },\n    { code: \"NO\", name: \"Norway\" },\n    { code