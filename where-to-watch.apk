\n// Clean up old entries every hour\nsetInterval(() =\u003e {\n  const now = Date.now()\n  for (const [ip, bucket] of buckets.entries()) {\n    // Remove entries older than 2 hours\n    if (now - bucket.lastRefill \u003e 7200000) {\n      buckets.delete(ip)\n    }\n  }\n}, 3600000) // 1 hour\n\ninterface RateLimitOptions {\n  maxTokens?: number // Maximum tokens in the bucket\n  refillRate?: number // Tokens per second to refill\n  tokensPerRequest?: number // Tokens consumed per request\n  blockDuration?: number // How long to block in ms after exceeding limit\n}\n\n// Default options\nconst defaultOptions: RateLimitOptions = {\n  maxTokens: 60, // 60 tokens max\n  refillRate: 1, // 1 token per second refill rate\n  tokensPerRequest: 1, // 1 token per request\n  blockDuration: 60000, // Block for 1 minute after exceeding\n}\n\n/**\n * Check if a request should be rate limited\n * @param ip IP address of the requester\n * @param endpoint Endpoint being accessed (for different limits)\n * @param options Rate limiting options\n * @returns Object with allowed status and headers\n */\nexport function checkRateLimit(\n  ip: string,\n  endpoint = \"default\",\n  options: RateLimitOptions = {},\n): {\n  allowed: boolean\n  remaining: number\n  resetAt: number\n  retryAfter?: number\n} {\n  // Use more strict limits for sensitive endpoints\n  let endpointOptions = { ...defaultOptions }\n\n  if (endpoint.includes(\"/api/cache\") || endpoint.includes(\"/api/update\") || endpoint.includes(\"/api/admin\")) {\n    // More strict limits for admin/cache endpoints\n    endpointOptions = {\n      maxTokens: 20,\n      refillRate: 0.2, // 1 token per 5 seconds\n      tokensPerRequest: 2,\n      blockDuration: 300000, // 5 minutes\n    }\n  } else if (endpoint.includes(\"/api/search\") || endpoint.includes(\"/api/autocomplete\")) {\n    // Search can be more frequent but still limited\n    endpointOptions = {\n      maxTokens: 40,\n      refillRate: 0.5, // 1 token per 2 seconds\n   