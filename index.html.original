      // Function to open success modal
      function openSuccessModal() {
        successModal.classList.remove('hidden');
        document.body.style.overflow = 'hidden';
        successModal.setAttribute('aria-hidden', 'false');
        
        // Animate modal appearing
        setTimeout(() => {
          successContent.classList.remove('scale-95', 'opacity-0');
          successContent.classList.add('scale-100', 'opacity-100');
        }, 10);
        
        // Set focus inside modal
        setTimeout(() => {
          confirmSuccessBtn.focus();
        }, 200);
      }
      
      // Function to close success modal
      function closeSuccessModal() {
        successContent.classList.remove('scale-100', 'opacity-100');
        successContent.classList.add('scale-95', 'opacity-0');
        
        setTimeout(() => {
          successModal.classList.add('hidden');
          document.body.style.overflow = '';
          successModal.setAttribute('aria-hidden', 'true');
        }, 200);
        
        // Return focus to the button that opened it
        askQuestionBtn.focus();
      }
      
      // Event listeners for modals
      askQuestionBtn.addEventListener('click', openAskModal);
      closeAskModalBtn.addEventListener('click', closeAskModal);
      closeSuccessModalBtn.addEventListener('click', closeSuccessModal);
      confirmSuccessBtn.addEventListener('click', closeSuccessModal);
      
      // Close when clicking outside modal
      askQuestionModal.addEventListener('click', function(e) {
        if (e.target === askQuestionModal) {
          closeAskModal();
        }
      });
      
      successModal.addEventListener('click', function(e) {
        if (e.target === successModal) {
          closeSuccessModal();
        }
      });
      
      // Close on ESC key
      document.addEventListener('keydown', function(e) {
        if (e.key === 'Escape') {
          if (!askQuestionModal.classList.contains('hidden')) {
            closeAskModal();
          }
          if (!successModal.classList.contains('hidden')) {
            closeSuccessModal();
          }
        }
      });
      
      // Trap focus inside modals
